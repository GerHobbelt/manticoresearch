<?xml version="1.0" encoding="utf-8"?>
<test>

<name>columnar packing</name>

<requires>
<columnar/>
</requires>

<config>
indexer
{
	mem_limit		= 16M
}

searchd
{
	<searchd_settings/>
}

source test255
{
	type			= mysql
	<sql_settings/>
	sql_query		= select * from test_table limit 255
	sql_field_string  = guid
}

source test256
{
	type			= mysql
	<sql_settings/>
	sql_query		= select * from test_table limit 256
	sql_field_string  = guid
}

index test255
{
	source 			= test255
	path			= <data_path/>/test255
	columnar_attrs 	= id, guid
}

index test256
{
	source 			= test256
	path			= <data_path/>/test256
	columnar_attrs 	= id, guid
}

source src_mva
{
	type			= mysql
	<sql_settings/>
	sql_query		= select * from test_mva
	sql_field_string  = guid
	sql_attr_multi	= uint mva from field
}

index test_mva
{
	source 			= src_mva
	path			= <data_path/>/test_mva
	columnar_attrs 	= *
}
</config>

<db_create>
CREATE TABLE `test_table` (
  `id` int(11),
  `guid` varchar(255)
);
</db_create>

<db_create>
CREATE TABLE `test_mva` (
  `id` int(11),
  `guid` varchar(255),
  `mva` varchar(255)
);
</db_create>

<db_drop>DROP TABLE IF EXISTS test_table;</db_drop>
<db_drop>DROP TABLE IF EXISTS test_mva;</db_drop>

